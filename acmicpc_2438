package study;

import java.util.Scanner;

public class judge_2438 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int num;
		num = input.nextInt();
		if(num < 1 || num > 100)
			return;
		
		for(int i=0;i<num;i++){
			for(int j=0;j<=i;j++){
				System.out.print("*");
			}
			System.out.println();
		}
		
	}

}
