package study;

import java.util.Scanner;

public class judge_1000 {
	public static void main(String[] args) {
		int a,b;
		Scanner scan1 = new Scanner(System.in);
		a = scan1.nextInt();
		b = scan1.nextInt();
		if(a>10 || a<0)
			a = scan1.nextInt();
		if(b>10 || b<0)
			b = scan1.nextInt();
		System.out.println(a+b);
		
	}	

}
